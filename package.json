{
  "name": "test",
  "version": "2.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "mocha src/**/*.spec.ts --require ts-node/register --reporter spec",
    "start": "node index.js",
    "watch": "tsc -w",
    "serve": "nodemon -e ts --watch src --exec \"npm run build && npm run start\" "
  },
  "dependencies": {
    "@nestjs/common": "^4.5.2",
    "@nestjs/core": "^4.5.2",
    "@nestjs/microservices": "^4.5.2",
