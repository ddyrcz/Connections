    "@nestjs/websockets": "^4.5.2",
    "body-parser": "^1.18.2",
    "mongoose": "^4.13.7",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/express": "^4.11.0",
    "@types/mocha": "^2.2.44",
    "@types/mongoose": "^4.7.30",
    "@types/node": "^8.5.2",
    "@types/sinon": "^2.3.7",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "nodemon": "^1.14.3",
    "sinon": "^2.4.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2"
  }
